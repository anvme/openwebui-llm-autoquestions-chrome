{
  "manifest_version": 3,
  "name": "WebUI Questions helper",
  "version": "1.0",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["*://ai.domain.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["q.json"],
      "matches": ["*://ai.domain.com/*"],
      "use_dynamic_url": true
    }
  ]
}